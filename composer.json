{
    "name": "coreboost/coreboost",
    "description": "WordPress performance optimization plugin",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "CoreBoost\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoreBoost\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html tests/coverage"
    }
}
